<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate><![CDATA[Vorlage ausw&auml;hlen]]></selectTemplate>
      <select-repository><![CDATA[Ablage ausw&auml;hlen]]></select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName><![CDATA[Arbeitsplatz ausw&auml;hlen]]></workspaceName>
      <savedPath>Ort der zu speichern ist</savedPath>
      <repositoryName><![CDATA[Ablage ausw&auml;hlen]]></repositoryName>
    </label>
    <msg>
      <save-successfully>Erfolgreich in Portleteinstellungen gespeichert</save-successfully>
      <item-not-found><![CDATA[Der Pfad ist ung&uuml;ltig!]]></item-not-found>
      <fileType-empty><![CDATA[Sie m&uuml;ssen einen Dateitypen w&auml;hlen bevor Sie diese Konfiguration speichern.]]></fileType-empty>
      <ws-empty><![CDATA[Sie m&uuml;ssen einen Arbeitsplatz aussuchen.]]></ws-empty>
    </msg>
    <action>
      <Save>Speichern</Save>
    </action>
  </UIEditModeConfiguration>
  <UIPopupWindow>
    <title>
      <UIOneNodePathSelector><![CDATA[Ort ausw&auml;hlen]]></UIOneNodePathSelector>
    </title>
  </UIPopupWindow>
  <UIFastContentCreatortForm>
    <msg>
      <name-not-allowed>This name is not allowed.</name-not-allowed>
      <in-versioning><![CDATA[Jetziger Knoten muss abgemeldet werden bevor ein neuer hinzugef&uuml;gt werden kann]]></in-versioning>
      <cannot-save><![CDATA[Einige Grenzen wurden &uuml;berschritten um diese Aktion auszuf&uuml;hren.]]></cannot-save>
      <saved-successfully>Dieser Knoten wurde erfolgreich am Ort '{0}' gespeichert</saved-successfully>
      <not-support><![CDATA[Die Vorlage f&uuml;r '{0}' kann nicht geladen werden.]]></not-support>
      <not-add-folder><![CDATA[Nur nt:Datei kann diesem Ordner zugef&uuml;gt werden]]></not-add-folder>
      <access-denied><![CDATA[Zugang verweigert! Sie haben keine Zuf&uuml;gegenehmigung in '{0}']]></access-denied>
      <path-not-found><![CDATA[Pfad '{0}' wurde gel&ouml;scht oder ist ung&uuml;ltig.]]></path-not-found>
      <node-locked><![CDATA[Knoten '{0}' ist gebunden. Knoten l&ouml;sen bevor Dokumenterstellung.]]></node-locked>
      <node-isExist>Dieser Knoten existiert schon in '{0}' und der gleiche Name kann nicht behalten werden.</node-isExist>
    </msg>
    <action>
      <Save>Speichern</Save>
      <Cancel>Abbrechen</Cancel>
    </action>
  </UIFastContentCreatortForm>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Typwert in das Feld '{0}' bitte</empty-input>
    </msg>
  </EmptyFieldValidator>
  <UITreeList>
    <info>
      <child-not-found>Dieser Knoten hat kein Kind</child-not-found>
    </info>
    <header>
      <name>Name</name>
      <action>Aktion</action>
    </header>
  </UITreeList>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete><![CDATA[Wollen Sie l&ouml;schen?]]></confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <UIFastContentCreatorPortlet>
    <msg>
      <re-config><![CDATA[&uuml;berpr&uuml;fen Sie an der Konfiguration. Vielleicht bestehen dort Probleme.]]></re-config>
    </msg>
  </UIFastContentCreatorPortlet>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Field '{0}' with value '{1}' is invalid</Invalid-input>
    </msg>
  </DateTimeValidator>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.ECMNameValidator																			  #
  ###################################################################################
  -->
  <ECMNameValidator>
    <msg>
      <Invalid-char><![CDATA[Es sind einige ung&uuml;ltige Karaktere im Feld '{0}'. Bitte neue Eingabe!]]></Invalid-char>
      <empty-input>Der Wert des Feldes '{0}' ist Null oder leer!</empty-input>
    </msg>
  </ECMNameValidator>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Arbeitsplatz</workspaceName>
      <rootNodeInfo>Wurzelpfad</rootNodeInfo>
      <rootPath>Wurzelpfad</rootPath>
    </label>
  </UIWorkspaceList>
  <!--
  # to be translated
  -->
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Select categories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Referenced categories</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Close</close>
    </action>
  </UICategoriesSelector>
</bundle>

