  -->
  <UIDocumentConfig>
    <title>Document Config</title>
    <label>
      <repository>Repository</repository>
      <workspace>Werkruimte</workspace>
      <categoryPath>Categoriespad</categoryPath>
      <path>UIDocumentConfig</path>
      <doc>UIDocumentConfig</doc>
      <docName>Documentnaam</docName>
