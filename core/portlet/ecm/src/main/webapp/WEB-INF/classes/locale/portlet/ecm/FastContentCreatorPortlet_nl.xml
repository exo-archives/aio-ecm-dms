<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate>Selecteer template</selectTemplate>
      <select-repository>Selecteer repository</select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName>Selecteer werkruimte</workspaceName>
      <savedPath>Locatie om op te slaan</savedPath>
      <repositoryName>Selecteer repository</repositoryName>
    </label>
    <msg>
      <save-successfully>Succesvol opgeslaan naar portlet voorkeuren</save-successfully>
      <item-not-found>Ongeldig pad</item-not-found>
      <fileType-empty>U moet eerst bestandstype kiezen voor u deze configuratie opslaat</fileType-empty>
      <ws-empty>U moet ��n werkruimte kiezen</ws-empty>
    </msg>
    <action>
      <Save>Opslaan</Save>
    </action>
  </UIEditModeConfiguration>
  <UIPopupWindow>
    <title>
      <UIOneNodePathSelector>Selecteer locatie</UIOneNodePathSelector>
    </title>
  </UIPopupWindow>
  <UIFastContentCreatortForm>
    <msg>
      <name-not-allowed>This name is not allowed.</name-not-allowed>
      <in-versioning>U moet huidige node uitchecken vooraleer u een nieuwe node toevoegd</in-versioning>
      <cannot-save>Sommige beperkingen werden overtreden tijdens die actie.</cannot-save>
      <saved-successfully>Node werd niet succesvol opgeslaan in '{0}' locatie</saved-successfully>
      <not-support>Kan template laden van '{0}'.</not-support>
      <not-add-folder>alleen nt:toevoegen van bestand in deze folder</not-add-folder>
      <access-denied>Toegang geweigerd! U hebt geen rechten in '{0}'</access-denied>
      <path-not-found>Pad '{0}' werd verwijderd of ongeldig pad</path-not-found>
      <node-locked>Node '{0}' is op slot. Maak node terug vrij voor het aanmaken van documenten</node-locked>
      <node-isExist>Node bestaat reeds in '{0}' en zelfde naam is niet toegelaten</node-isExist>
    </msg>
    <action>
      <Save>Opslaan</Save>
      <Cancel>Annuleren</Cancel>
    </action>
  </UIFastContentCreatortForm>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Geef waarde in veld '{0}'</empty-input>
    </msg>
  </EmptyFieldValidator>
  <UITreeList>
    <info>
      <child-not-found>Node heeft geen enkele child</child-not-found>
    </info>
    <header>
      <name>Naam</name>
      <action>Actie</action>
    </header>
  </UITreeList>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Wilt u dit verwijderen?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <UIFastContentCreatorPortlet>
    <msg>
      <re-config>Controleer uw configuratie. U hebt misschien problemen met deze configuratie</re-config>
    </msg>
  </UIFastContentCreatorPortlet>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Veld '{0}' met waarde '{1}' is ongeldig</Invalid-input>
    </msg>
  </DateTimeValidator>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.ECMNameValidator																			  #
  ###################################################################################
  -->
  <ECMNameValidator>
    <msg>
      <Invalid-char>Er zijn ongeldige karakters in veld '{0}'. Geef een andere waarde in.</Invalid-char>
      <empty-input>Waarde van veld '{0}' is null of leeg!</empty-input>
    </msg>
  </ECMNameValidator>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Werkruimte</workspaceName>
      <rootNodeInfo>Root pad</rootNodeInfo>
    </label>
  </UIWorkspaceList>
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Select categories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Referenced categories</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Close</close>
    </action>
  </UICategoriesSelector>
</bundle>

