<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate>Select template</selectTemplate>
      <select-repository>Select repository</select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName>Select workspace</workspaceName>
      <savedPath>Location to save</savedPath>
      <repositoryName>Select repository</repositoryName>
    </label>
    <msg>
      <save-successfully>Saved to portlet preferences successfully</save-successfully>
      <item-not-found>This path is invalid!</item-not-found>
      <fileType-empty>You have to choose file type before save this configuration.</fileType-empty>
      <ws-empty>You have to choose one workspace.</ws-empty>
    </msg>
    <action>
      <Save>Save</Save>
    </action>
  </UIEditModeConfiguration>
  <UIPopupWindow>
    <title>
      <UIOneNodePathSelector>Select location</UIOneNodePathSelector>
    </title>
  </UIPopupWindow>
  <UIFastContentCreatortForm>
    <msg>
      <name-not-allowed>This name is not allowed.</name-not-allowed>
      <in-versioning>Need to checkout current node before add new node</in-versioning>
      <cannot-save>Some constraints were violated during that action.</cannot-save>
      <saved-successfully>This node has been saved at '{0}' location successfully</saved-successfully>
      <not-support>Cannot load template of '{0}'.</not-support>
      <not-add-folder>Only nt:file can add in this folder</not-add-folder>
      <access-denied>Access denied! You haven't got add permission at '{0}'</access-denied>
      <path-not-found>Path '{0}' was deleted or is invalid path.</path-not-found>
      <node-locked>Node '{0}' is locked. Unlock this node before create documents.</node-locked>
      <node-isExist>This node already exists in '{0}' and same name sibling is not allowed</node-isExist>
    </msg>
    <action>
      <Save>Save</Save>
      <Cancel>Cancel</Cancel>
    </action>
  </UIFastContentCreatortForm>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Type value into field '{0}' please</empty-input>
    </msg>
  </EmptyFieldValidator>
  <UITreeList>
    <info>
      <child-not-found>This node hasn't got any child</child-not-found>
    </info>
    <header>
      <name>Name</name>
      <action>Action</action>
    </header>
  </UITreeList>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Do you want to delete?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <UIFastContentCreatorPortlet>
    <msg>
      <re-config>Check with your configuration. Maybe have some problems with this config.</re-config>
    </msg>
  </UIFastContentCreatorPortlet>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Field '{0}' with value '{1}' is invalid</Invalid-input>
    </msg>
  </DateTimeValidator>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.ECMNameValidator																			  #
  ###################################################################################
  -->
  <ECMNameValidator>
    <msg>
      <Invalid-char>There are some invalid characters in field '{0}'. Type another value, please!</Invalid-char>
      <empty-input>The value of field '{0}' is null or empty!</empty-input>
    </msg>
  </ECMNameValidator>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodeInfo>Root path</rootNodeInfo>
    </label>
  </UIWorkspaceList>
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Select categories</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Referenced categories</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Add Categories</UICategoriesSelector>
    </title>
  </PopupComponent>
  
  <UICategoriesSelector>
    <action>
      <close>Close</close>
    </action>
  </UICategoriesSelector>

  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Add Categories</UIOneTaxonomySelector>
    </title>
  </PopupComponent>
  
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Taxonomy Tree</taxonomyTree>
    </label>
  </UITreeTaxonomyList>
  
  <UIDocumentForm>
  	<msg>
  		<not-support>Cannot load template of '{0}'.</not-support>
  		<do-not-permission>You don't have permission to load template of '{0}'.</do-not-permission>
  	</msg>
  </UIDocumentForm>
</bundle>

