<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################################
  #Edit mode																		  #
  ###################################################################################
  -->
  <UIEditModeConfiguration>
    <label>
      <selectTemplate>Định dạng</selectTemplate>
      <select-repository>Kho chứa</select-repository>
      <actionInput>UIEditModeConfiguration</actionInput>
      <workspaceName>Vùng làm việc</workspaceName>
      <savedPath>Định vị vị trí để lưu</savedPath>
      <repositoryName>Lựa chọn kho chứa</repositoryName>
    </label>
    <msg>
      <save-successfully>Việc cấu hình porlet đã thành công</save-successfully>
      <item-not-found>Đường dẫn này không đúng!</item-not-found>
      <fileType-empty>Bạn phải chọn kiểu tài liệu trước khi lưu lại cấu hình này.</fileType-empty>
      <ws-empty>Bạn phải chọn một vùng làm việc.</ws-empty>
    </msg>
    <action>
      <Save>Lưu</Save>
    </action>
  </UIEditModeConfiguration>
  <UIPopupWindow>
    <title>
      <UIOneNodePathSelector>Định vị vị trí</UIOneNodePathSelector>
    </title>
  </UIPopupWindow>
  <UIFastContentCreatortForm>
    <msg>
      <name-not-allowed>Tên này không được chấp nhận.</name-not-allowed>
      <in-versioning>Bạn cần check out node hiện tại trước khi thêm node con cho nó</in-versioning>
      <cannot-save>Một vài ràng buộc bị vi phạm trong quá trình thực hiện hành động.</cannot-save>
      <saved-successfully>Nút này đã được lưu thành công tại vị trí '{0}'</saved-successfully>
      <not-support>Không thể tải được định dạng của '{0}'.</not-support>
      <not-add-folder>Chỉ chấp nhận thêm kiểu nt:file trong thư mục này</not-add-folder>
      <access-denied>Bạn không có quyền để tạo mới một node trong '{0}'</access-denied>
      <path-not-found>Đường dẫn '{0}' đã bị xóa hoặc đó là đường dẫn không đúng.</path-not-found>
      <node-locked>Nút '{0}' đã bị khóa. Nút phải được mở khóa trước khi tạo tài liệu.</node-locked>
      <node-isExist>Nút này đã tồn tại trong '{0}' và tên cùng mức không được chấp nhận</node-isExist>
    </msg>
    <action>
      <Save>Lưu</Save>
      <Cancel>Hủy</Cancel>
    </action>
  </UIFastContentCreatortForm>
  <EmptyFieldValidator>
    <msg>
      <empty-input>Hãy nhập giá trị cho trường '{0}'</empty-input>
    </msg>
  </EmptyFieldValidator>
  <UITreeList>
    <info>
      <child-not-found>Nút này không chứa con nào</child-not-found>
    </info>
    <header>
      <name>Tên</name>
      <action>Hành động</action>
    </header>
  </UITreeList>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Bạn có muốn xóa không?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <UIFastContentCreatorPortlet>
    <msg>
      <re-config>Kiểm tra lại cấu hình của bạn. Có thể có một vài vấn đề với cấu hình này.</re-config>
    </msg>
  </UIFastContentCreatorPortlet>
  <DateTimeValidator>
    <msg>
      <Invalid-input>Trường '{0}' với giá trị '{1}' là không hợp lệ</Invalid-input>
    </msg>
  </DateTimeValidator>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.ECMNameValidator																			  #
  ###################################################################################
  -->
  <ECMNameValidator>
    <msg>
      <Invalid-char>Có một vài ký tự không hợp lệ trong trường '{0}'. Xin hãy nhập giá trị khác!</Invalid-char>
      <empty-input>Giá trị của trường '{0}' là trống rỗng!</empty-input>
    </msg>
  </ECMNameValidator>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Vùng làm việc</workspaceName>
      <rootNodeInfo>Đường dẫn gốc</rootNodeInfo>
    </label>
  </UIWorkspaceList>
  <UITabPane>
    <title>
      <UICategoriesSelectPanel>Lựa chọn loại tài liệu</UICategoriesSelectPanel>
      <UISelectedCategoriesGrid>Loại tài liệu liên quan</UISelectedCategoriesGrid>
    </title>
  </UITabPane>
  <PopupComponent>
    <title>
      <UICategoriesSelector>Thêm loại tài liệu</UICategoriesSelector>
    </title>
  </PopupComponent>
  <UICategoriesSelector>
    <action>
      <close>Đóng</close>
    </action>
  </UICategoriesSelector>
  
    <PopupComponent>
	  <title>
	    <UIOneTaxonomySelector>Thêm danh mục</UIOneTaxonomySelector>
	  </title>
  </PopupComponent>
  
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Cây danh mục</taxonomyTree>
    </label>
  </UITreeTaxonomyList>
  
  <UIDocumentForm>
  	<msg>
  		<not-support>Cannot load template of '{0}'.</not-support>
  		<do-not-permission>You don't have permission to load template of '{0}'.</do-not-permission>
  	</msg>
  </UIDocumentForm>
</bundle>

