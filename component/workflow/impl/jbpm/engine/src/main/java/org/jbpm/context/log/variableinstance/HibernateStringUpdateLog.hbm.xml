<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">

  <subclass name="org.jbpm.context.log.variableinstance.HibernateStringUpdateLog" 
            extends="org.jbpm.context.log.VariableUpdateLog"
            discriminator-value="F">
    <any name="oldValue" id-type="string" cascade="save-update">
      <column name="OLDSTRINGIDCLASS_" />
      <column name="OLDSTRINGIDVALUE_" />
    </any>
    <any name="newValue" id-type="string" cascade="save-update">
      <column name="NEWSTRINGIDCLASS_" />
      <column name="NEWSTRINGIDVALUE_" />
    </any>
  </subclass>

</hibernate-mapping>
